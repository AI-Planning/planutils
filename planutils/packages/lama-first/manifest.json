{
    "name": "LAMA-FIRST",
    "description": "http://fast-downward.org/",
    "install-size": "20K",
    "dependencies": ["downward"],

    "endpoint": {
        "type": "solver",
        "services": {
            "solve": {
                "call":"lama-first {domain} {problem}",
                "return": {
                    "type": "generic",
                    "file": "sas_plan"
                }
            }
        }
    }
}