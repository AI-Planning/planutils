{
    "name": "RP-MEP",
    "description": "Epistemic Planner RP-MEP. https://github.com/QuMuLab/pdkb-planning",
    "install-size": "255M",
    "dependencies": [],
    "endpoint": {
      "services": {
        "solve": {
          "template": "planner",
          "call":"{package_name} {domain} {problem} --keep-files",
          "return": {
            "type": "generic",
            "files": "pdkb-*"
          }
        }
      }
    }
}
